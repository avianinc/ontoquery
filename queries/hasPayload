PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX ex: <http://www.semanticweb.org/johndehart/ontologies/2024/8/untitled-ontology-47#>

# Query to retrieve all subjects and objects linked by the 'hasPayload' property
SELECT ?subject ?payload
WHERE { 
  ?subject ex:hasPayload ?payload .
}
